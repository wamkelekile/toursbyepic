<!DOCTYPE HTML>
<html lang="en">
<head>
    <!--=============== basic  ===============-->
    <meta charset="UTF-8">
    <title>{% block title %}  {% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="robots" content="index, follow"/>
    <meta name="keywords" content=""/>
    <meta name="description" content=""/>

    <!--=============== css  ===============-->
    {% block stylesheets %}
    <link type="text/css" rel="stylesheet" href="{{ asset('css/reset.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('css/plugins.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('css/style.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('css/color.css') }}">
    <link type="text/css" rel="stylesheet" href="{{ asset('css/custom.css') }}">
    {% endblock %}

    <!--=============== favicons ===============-->
    <link rel="shortcut icon" href="{{ asset('images/favicon.ico') }}">
</head>
<body>
<!--loader-->
<div class="loader-wrap">
    <div class="pin">
        <div class="pulse"></div>
    </div>
</div>
<!--loader end-->
<!-- Main  -->
<div id="main">
    <!-- header-->
    {% include 'includes/header.html.twig' %}
    <!--  header end -->

    {% block body %}

    {% endblock %}

    <!--footer -->
    {% include 'includes/footer.html.twig' %}
    <!--footer end -->
    <a class="to-top"><i class="fas fa-caret-up"></i></a>
</div>
<!-- Main end -->

<!--=============== scripts  ===============-->
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('js/jquery.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/plugins.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/scripts.js') }}"></script>
{#
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAhG0touuyZrk6lACS1GIACA0Ng-FoWQjw&libraries=places&callback=initAutocomplete"></script>
#}
    <script type="text/javascript" src="{{ asset('js/map-single.js') }}"></script>
{% endblock %}

</body>
</html>